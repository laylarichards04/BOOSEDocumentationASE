{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "..\\repos\\ase-boose-assignment-laylarichards04\\BOOSEAppASE",
          "files": ["**/*.csproj"]
        },
        {
          "src": "..\\repos\\ase-boose-assignment-laylarichards04\\BOOSETest",
          "files": ["**/*.csproj"]
        }
      ],
      "dest": "api",
      
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**/*.{md,yml}"],
        "exclude": ["_site/**"]
      }
    ],
    "resource": [
      { "files": ["images/**"] }
    ],
    "globalMetadata": {
      "_appName": "BOOSEDocSite",
      "_appTitle": "BOOSEDocSite",
      "_enableSearch": true,
      "pdf": false
    },
    "template": ["default", "modern"],
    "output": "_site"
  }
}
